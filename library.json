{
    "name": "roo_scheduler",
    "description": "Arduino-compliant microcontroller library for scheduling delayed and/or repetitive tasks. Works on Espressif boards (e.g. ESP32).",
    "keywords": ["tasks", "scheduling", "periodic", "delayed"],
    "authors": [
        {
            "name": "Dawid Kurzyniec",
            "email": "dawid.kurzyniec@gmail.com",
            "maintainer": true
        }
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/dejwk/roo_scheduler"
    },
    "version": "1.0.2",
    "homepage": "https://github.com/dejwk/roo_scheduler",
    "frameworks": [
        "Arduino",
        "*"
    ],
    "build": {
        "srcDir": "src",
        "srcFilter": "+<*> -<examples> -<test>"
    },
    "examples": [
        "examples/*/*.ino"
    ],
    "dependencies": [
        {
            "owner": "dejwk",
            "name": "roo_time",
            "version": "^1.0.1"
        }
    ]
}
